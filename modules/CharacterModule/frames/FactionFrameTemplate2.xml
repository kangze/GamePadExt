<Ui xmlns="http://www.blizzard.com/wow/ui/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">
    <Script file="FactionFrameTemplate2Mixin.lua"/>
    <Frame name="FactionFrameTemplate2" virtual="true" frameStrata="HIGH" enableMouse="true" mixin="FactionFrameTemplate2Mixin">
        <Layers>
            <Layer level="BACKGROUND">
                <Texture parentKey="background" atlas="talents-background-rogue-assassination">
                </Texture>
            </Layer>
        </Layers>
        <Frames>
            <Frame parentKey="title1" inherits="GpeExpansionItemTemplate">
                <Size x="190" y="50"/>
                <Anchors>
                    <Anchor point="TOPLEFT" x="50" y="-50"/>
                </Anchors>
            </Frame>

            <Frame parentKey="faction1" inherits="GpeFactionItemTemplate">
                <Size x="220" y="50"/>
                <Anchors>
                    <Anchor point="CENTER"/>
                </Anchors>
            </Frame>

            <Frame parentKey="currency" inherits="GpeCurrencyItemTemplate">
                <Size x="200" y="55"/>
                <Anchors>
                    <Anchor point="BOTTOM" y="40"/>
                </Anchors>
            </Frame>

        </Frames>
    </Frame>
</Ui>