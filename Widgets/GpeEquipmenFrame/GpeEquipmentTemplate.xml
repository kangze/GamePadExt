<Ui xmlns="http://www.blizzard.com/wow/ui/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">
    <Script file="GpeEquipmentTemplateMixin.lua"/>
    <ItemButton name="GpeEquipmentTemplate" enableMouse="true" virtual="true" mixin="GpeEquipmentTemplateMixin">
        <Size x="40" y="40"/>
        <Layers>
            <Layer level="ARTWORK">
                <FontString parentKey="name" font="Fonts/FRIZQT__.TTF" outline="THICKOUTLINE" inherits="GameFontNormalLeft" justifyH="LEFT" justifyV="TOP">
                    <FontHeight val="11"/>
                    <Anchors>
                        <Anchor point="BOTTOMLEFT" relativePoint="BOTTOMRIGHT" x="5"/>
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
        <Scripts>
            <OnLoad method="OnLoad"/>
            <OnEnter method="OnEnter"/>
            <OnLeave method="OnLeave"/>
        </Scripts>
    </ItemButton>
</Ui>